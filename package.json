{
  "name": "express-ts-template",
  "version": "1.0.0",
  "description": "A minimal template for express typescript",
  "scripts": {
    "build": "tsup src/server.ts --out-dir dist --format esm",
    "clean": "rm -rf ./dist",
    "start": "node ./dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "db:migrate": "npx knex migrate:latest",
    "db:rollback": "npx knex migrate:rollback",
    "db:make": "npx knex migrate:make",
    "db:seed": "npx knex seed:run",
    "db:types": "kysely-codegen --dialect mysql --out-file src/db/types.ts",
    "db:refresh": "pnpm db:migrate && pnpm db:types",
    "pretty": "prettier --write 'src/**/*.ts'",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.6",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.7",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "mysql2": "^3.15.0",
    "nanoid": "^5.1.5",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "tsup": "^8.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.19",
    "eslint": "^9.27.0",
    "jest": "^30.2.0",
    "knex": "^3.1.0",
    "kysely-codegen": "^0.20.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
