{
  "name": "mlm-backend",
  "version": "1.0.0",
  "description": "A backend service for a multi-level marketing application",
  "type": "module",
  "scripts": {
    "build": "tsup src/server.ts --out-dir dist --format esm",
    "build:tsup": "tsup",
    "build:safe": "tsup src/server.ts --format esm --out-dir dist_new && mv dist dist_old && mv dist_new dist && rm -rf dist_old",
    "clean": "rm -rf ./dist",
    "start": "./dist/server.js",
    "dev": "tsx watch src/server.ts",
    "db:init": "npx prisma init",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "db:format": "prisma format",
    "db:reset": "prisma migrate reset",
    "db:seed-admin": "tsx src/scripts/create-admin.ts",
    "db:seed-packages": "tsx src/scripts/create-packages.ts",
    "db:seed-levels": "tsx src/scripts/create-levels.ts",
    "db:seed-accounts": "tsx src/scripts/create-accounts.ts",
    "db:seed-perms": "tsx src/scripts/create-permissions.ts",
    "worker:default": "tsx src/workers/default.ts",
    "worker:critical": "tsx src/workers/critical.ts",
    "worker:batch": "tsx src/workers/batch.ts",
    "worker:start": "pm2 start ecosystem.config.js",
    "db:seed-cat": "tsx src/scripts/create-category.ts",
    "db:seed-ranks": "tsx src/scripts/create-ranks.ts",
    "db:seed-set": "tsx src/scripts/create-settings.ts",
    "db:seed-emails": "tsx src/scripts/create-emails.ts",
    "db:seed-sms": "tsx src/scripts/create-sms.ts",
    "clear:cache": "tsx src/scripts/clear-cache.ts",
    "clear:queue": "tsx src/scripts/clear-queue.ts",
    "cli:test": "tsx src/cli/test.ts",
    "cli:unreleased-commissions": "tsx src/cli/commissions/disburse-unreleased.ts",
    "command": "tsx src/cli.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ably/chat": "^0.11.0",
    "@eslint/js": "^9.31.0",
    "@faker-js/faker": "^9.9.0",
    "@prisma/client": "^6.12.0",
    "@types/lodash": "^4.17.20",
    "@types/qs": "^6.14.0",
    "ably": "^2.10.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.7",
    "commander": "^14.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.3",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "nanoid": "^5.1.5",
    "node-cache": "^5.1.2",
    "pino": "^9.7.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^13.0.0",
    "prisma-kysely": "^1.8.0",
    "redis": "^5.6.1",
    "tsup": "^8.5.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@sentry/types": "^9.40.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.16.5",
    "@types/validator": "^13.15.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "overrides": {
    "rimraf": "^5.0.0",
    "glob": "^10.4.1",
    "fstream": ">=1.0.12",
    "inflight": ">=1.0.6",
    "lodash.isequal": "^4.5.0"
  }
}
